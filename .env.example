# Database
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# AI Services
OPENAI_API_KEY=your_openai_api_key
REPLICATE_API_TOKEN=your_replicate_api_token

# Authentication
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Session Management Configuration
# Options: 'default', 'strict', 'relaxed'
# - default: 2h inactivity, 24h max session (recommended for production)
# - strict: 15min inactivity, 4h max session (high security environments)
# - relaxed: 8h inactivity, 72h max session (development/testing)
NEXT_PUBLIC_SESSION_CONFIG=default

# Security Configuration
NEXT_PUBLIC_ENABLE_MFA=true
NEXT_PUBLIC_ENABLE_RATE_LIMITING=true
NEXT_PUBLIC_MAX_LOGIN_ATTEMPTS=5
NEXT_PUBLIC_LOCKOUT_DURATION_MINUTES=15

# Analytics
GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Stripe (for payments)
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Email
RESEND_API_KEY=your_resend_api_key
FROM_EMAIL=noreply@yourdomain.com

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=IBIAS